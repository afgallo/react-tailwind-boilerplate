{
  "name": "jotai-tailwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "dependencies": {
    "@craco/craco": "6.1.2",
    "@headlessui/react": "1.2.0",
    "@heroicons/react": "1.0.1",
    "@tailwindcss/forms": "0.3.3",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "jotai": "1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "1.1.2"
  },
  "devDependencies": {
    "autoprefixer": "9.8.6",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "postcss": "7.0.36",
    "prettier": "2.3.1",
    "prettier-eslint": "12.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.1.4"
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "semi": 0
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "printWidth": 120
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
